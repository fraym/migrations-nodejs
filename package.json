{
	"name": "@fraym/migrations",
	"version": "0.2.0",
	"license": "MIT",
	"homepage": "https://github.com/fraym/migrations-nodejs",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fraym/migrations-nodejs.git"
	},
	"description": "nodejs client implementation for our migrations service",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 0",
		"format": "prettier --write \"**/*.{ts,tsx,json}\"",
		"lint": "prettier --check \"**/*.{ts,tsx,json}\"",
		"build": "npm run clean && tsc && chmod +x dist/cmd/cmd.js",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm test && npm run lint && npm run build",
		"preversion": "npm run lint",
		"cmd": "dist/cmd/cmd.js"
	},
	"files": [
		"dist/**/*"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"migration": "dist/cmd/cmd.js"
	},
	"dependencies": {
		"@graphql-tools/import": "^7.0.0",
		"@graphql-tools/load": "^7.8.13",
		"dotenv": "^16.0.3",
		"glob": "^10.3.3",
		"globby": "^11.0.3",
		"graphql": "^16.6.0",
		"ts-node": "^10.9.1",
		"unixify": "^1.0.0",
		"ws": "^8.14.1",
		"yargs": "^17.7.1"
	},
	"devDependencies": {
		"@becklyn/prettier": "^1.0.2",
		"@types/node": "^20.2.3",
		"@types/unixify": "^1.0.0",
		"@types/uuid": "^9.0.1",
		"@types/ws": "^8.5.5",
		"@types/yargs": "^17.0.24",
		"prettier": "^2.8.7",
		"typescript": "^5.0.3",
		"uuid": "^9.0.0"
	},
	"prettier": "@becklyn/prettier"
}
